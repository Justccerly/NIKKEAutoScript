{
  "name": "NKAS",
  "private": false,
  "version": "0.0.6",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "electron:build": "vite build && electron-builder"
  },
  "build": {
    "appId": "NIKKE-Auto-Script",
    "productName": "NKAS",
    "win": {
      "icon": "src/assets/ico/Helm.ico",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^1.2.1",
    "dayjs": "^1.11.7",
    "electron-win-state": "^1.1.22",
    "element-plus": "^2.2.27",
    "got": "^11.8.6",
    "image-type": "^4.1.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.28",
    "pinia-plugin-persistedstate": "^3.0.1",
    "randomstring": "^1.2.3",
    "socket.io-client": "^4.5.4",
    "stylus": "^0.59.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "lodash": "^4.17.21",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-electron": "^0.11.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.11"
  }
}
